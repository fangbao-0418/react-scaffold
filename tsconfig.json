{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "target": "es5",
    "module": "es6",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "dom.iterable",
      "es2015",
      "es2016",
      "es2017"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}
